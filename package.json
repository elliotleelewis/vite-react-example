{
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc --noEmit && vite build",
    "build:preview": "vite preview",
    "prettier": "prettier --check \"**/*.{md,ts,js,tsx,jsx,scss,html,json,yml}\" --ignore-path .gitignore",
    "prettier:fix": "prettier --write \"**/*.{md,ts,js,tsx,jsx,scss,html,json,yml}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.0.0",
    "prettier": "~2.5.0",
    "tslib": "~2.3.0",
    "typescript": "~4.5.0",
    "vite": "2.0.0"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "overrides": [
      {
        "files": [
          "*.json",
          "*.md",
          "*.yml"
        ],
        "options": {
          "tabWidth": 2,
          "useTabs": false
        }
      }
    ],
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": true
  }
}
